{
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "plugins": [
    "jest"
  ],
  "settings": {},
  "rules": {
    "curly": "off",
    "guard-for-in": "off",
    "indent": "off",
    "linebreak-style": "off",
    "max-len": "off", 
    "no-console": "off",
    "no-duplicate-imports": [
      "error",
      {
        "includeExports": false
      }
    ],
    "no-negated-condition" : "off",
    "no-nested-ternary": "off",
    "no-throw-literal": "off",
    "no-undef": "warn",
    "no-unused-vars": "warn",
    "no-warning-comments": "warn",
    "quote-props": "warn",
    "padded-blocks": "warn",
    "radix": "off",
    "semi": ["error", "always", {"omitLastInOneLineBlock": true}],
    "space-before-function-paren": "off"
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "modules": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest/globals": true
  }
}

